{
  "name": "copy-of-travel-buddy-web-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "vite --host 0.0.0.0",
    "build": "vite build --mode production",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.json",
    "build:stats": "vite build --mode production --reporter verbose",
    "preview": "vite preview",
    "start": "node startup.js",
    "simple": "node simple-server.js",
    "azure:build": "npm run build",
    "azure:start": "npm run start",
    "deploy:prepare": "npm run build && npm run optimize",
    "deploy:azure": "powershell -ExecutionPolicy Bypass -File deploy-azure.ps1",
    "copy-webconfig": "cpx web.config dist/ && cpx sw.js dist/ && cpx public/favicon.ico dist/",
    "optimize": "npm run build && npm run analyze-build",
    "analyze-bundle": "echo 'Bundle analysis via analyze-build script'",
    "analyze-build": "node scripts/vite-build-analyzer.js",
    "build-summary": "node scripts/vite-build-analyzer.js",
    "health-check": "node scripts/health-check.js",
    "deploy:check": "npm run health-check",
    "diagnose": "node scripts/diagnose-azure.js",
    "admin:promote": "node scripts/promote-admin.js",
    "preload-critical": "node scripts/preload-critical-assets.js",
    "compress": "gzip -9 dist/assets/*.js dist/assets/*.css",
    "perf-test": "lighthouse http://localhost:5173 --output json --output-path lighthouse-report.json",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@google/genai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@google/maps": "^1.1.3",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.0.2",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongoose": "^8.0.3",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^22.14.0",
    "@types/socket.io-client": "^3.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "cpx": "^1.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "~5.7.2"
  }
}
